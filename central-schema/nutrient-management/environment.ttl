ns:Nutrient a owl:Class ;
    rdfs:label "Nutrient" .

ns:has_Sampling_Depth a owl:ObjectProperty ;
    rdfs:label "has sampling depth" .

ns:has_component a owl:ObjectProperty ;
    rdfs:label "has component" .

ns:has_nutrient_status a owl:ObjectProperty ;
    rdfs:label "has nutrient status" .

ns:has_soil_trait a owl:ObjectProperty ;
    rdfs:label "has soil trait" .

ns:Soil_Component a owl:Class ;
    rdfs:label "Soil Component" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns:has_Sampling_Depth ;
            owl:someValuesFrom ns:Sampling_Depth ],
        ns:Environment_Component .

ns:Vapor_Pressure_Deficit a owl:Class ;
    rdfs:label "Vapor Pressure Deficit" ;
    rdfs:subClassOf ns:Temperature .

ns:Location_Component a owl:Class ;
    rdfs:label "Location Component" ;
    rdfs:subClassOf ns:Environment_Component .

ns:Sampling_Depth a owl:Class ;
    rdfs:label "Sampling Depth" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns:has_nutrient_status ;
            owl:someValuesFrom ns:Nutrient ],
        [ a owl:Restriction ;
            owl:onProperty ns:has_soil_trait ;
            owl:someValuesFrom ns:Soil_Trait ],
        ns:Soil_Component .

ns:Weather_Component a owl:Class ;
    rdfs:label "Weather Component" ;
    rdfs:subClassOf ns:Environment_Component .

ns:Environment_Component a owl:Class .

ns:Experimental_Component a owl:Class ;
    rdfs:label "Experimental Component" ;
    rdfs:subClassOf ns:Environment_Component .

ns:Temperature a owl:Class ;
    rdfs:label "Temperature" ;
    rdfs:subClassOf ns:Weather_Component .

ns:Soil_Texture a owl:Class ;
    rdfs:label "Soil Texture" ;
    rdfs:subClassOf ns:Soil_Trait .

ns:Soil_Trait a owl:Class ;
    rdfs:label "Soil Trait" ;
    rdfs:subClassOf ns:Soil_Component .

