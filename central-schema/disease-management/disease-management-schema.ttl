@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ns: <https://gwsp.cs.ksu.edu/disease-management/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ns: a owl:Ontology ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> .

ns:Bacteria a owl:Class ;
    rdfs:label "Bacteria" ;
    rdfs:subClassOf ns:Pathogen .

ns:Disease_Management a owl:Class ;
    rdfs:label "Disease Management" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns:depends_on ;
            owl:someValuesFrom ns:Pathogen ] .

ns:Fungi a owl:Class ;
    rdfs:label "Fungi" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns:has_treatment_strategy ;
            owl:someValuesFrom ns:Host_Genetics ],
        [ a owl:Restriction ;
            owl:onProperty ns:has_treatment_strategy ;
            owl:someValuesFrom ns:Chemical_Strategy ],
        [ a owl:Restriction ;
            owl:onProperty ns:has_treatment_strategy ;
            owl:someValuesFrom ns:Cultural_Practice ],
        ns:Pathogen .

ns:Virus a owl:Class ;
    rdfs:label "Virus" ;
    rdfs:subClassOf ns:Pathogen .

<https://creativecommons.org/licenses/by/4.0/> dcterms:title "Creative Commons Attribution 4.0 International"@en .

ns:Chemical_Strategy a owl:Class ;
    rdfs:label "Chemical Strategy" .

ns:Cultural_Practice a owl:Class ;
    rdfs:label "Cultural Practice" .

ns:Host_Genetics a owl:Class ;
    rdfs:label "Host Genetics" .

ns:depends_on a owl:ObjectProperty ;
    rdfs:label "depends on" .

ns:has_treatment_strategy a owl:ObjectProperty ;
    rdfs:label "has treatment strategy" .

ns:Pathogen a owl:Class ;
    rdfs:label "Pathogen" .

