@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unl_KS: <https://gwsp.cs.ksu.edu/unlclimate/> .
@prefix uo: <http://purl.obolibrary.org/obo/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

unl_KS:Climate a rdfs:Class ;
    rdfs:label "Climate"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty unl_KS:hasMaxTemp ;
            owl:someValuesFrom unl_KS:MaxTemp ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasSnowfall ;
            owl:someValuesFrom unl_KS:Snowfall ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasPrecipitation ;
            owl:someValuesFrom unl_KS:Precipitation ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasMinTemp ;
            owl:someValuesFrom unl_KS:MinTemp ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasSnowDepth ;
            owl:someValuesFrom unl_KS:SnowDepth ] .

unl_KS:Date a rdfs:Class ;
    rdfs:label "Date"^^xsd:string .

unl_KS:MaxTemp a rdfs:Class ;
    rdfs:label "MaxTemp"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue uo:UO_0000028 ;
            owl:onProperty unl_KS:hasUnit ] .

unl_KS:MinTemp a rdfs:Class ;
    rdfs:label "MinTemp"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue uo:UO_0000028 ;
            owl:onProperty unl_KS:hasUnit ] .

unl_KS:Precipitation a rdfs:Class ;
    rdfs:label "Precipitation"^^xsd:string ;
    rdfs:comment "T: Trace amount — less than 0.01\". S: Subsequent — missing but included in a subsequent value. A: Accumulated — includes current day and any immediately preceding missing days (starting at an \"S\" day). M: Missing data."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue uo:UO_0000030 ;
            owl:onProperty unl_KS:hasUnit ] .

unl_KS:SnowDepth a rdfs:Class ;
    rdfs:label "SnowDepth"^^xsd:string ;
    rdfs:comment "T: Trace amount — less than 1\". S: Subsequent — missing but included in a subsequent value. A: Accumulated — includes current day and any immediately preceding missing days (starting at an \"S\" day). M: Missing data."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue uo:UO_0000030 ;
            owl:onProperty unl_KS:hasUnit ] .

unl_KS:Snowfall a rdfs:Class ;
    rdfs:label "Snowfall"^^xsd:string ;
    rdfs:comment "T: Trace amount — less than 0.1\". S: Subsequent — missing but included in a subsequent value. A: Accumulated — includes current day and any immediately preceding missing days (starting at an \"S\" day). M: Missing data."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue uo:UO_0000030 ;
            owl:onProperty unl_KS:hasUnit ] .

unl_KS:State a rdfs:Class ;
    rdfs:label "State"^^xsd:string .

unl_KS:Station a rdfs:Class ;
    rdfs:label "Station"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty unl_KS:hasState ;
            owl:someValuesFrom unl_KS:State ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasId ;
            owl:someValuesFrom unl_KS:StationID ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasRecordingDate ;
            owl:someValuesFrom unl_KS:Date ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasClimate ;
            owl:someValuesFrom unl_KS:Climate ],
        [ a owl:Restriction ;
            owl:onProperty unl_KS:hasName ;
            owl:someValuesFrom unl_KS:StationName ] .

unl_KS:StationID a rdfs:Class ;
    rdfs:label "StationID"^^xsd:string .

unl_KS:StationName a rdfs:Class ;
    rdfs:label "StationName"^^xsd:string .

unl_KS: a owl:Ontology ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> .

unl_KS:hasValue a owl:ObjectProperty ;
    rdfs:label "hasValue"^^xsd:string .

<https://creativecommons.org/licenses/by/4.0/> dcterms:title "Creative Commons Attribution 4.0 International"@en .

unl_KS:hasClimate a owl:ObjectProperty ;
    rdfs:label "hasClimate"^^xsd:string .

unl_KS:hasId a owl:ObjectProperty ;
    rdfs:label "hasId"^^xsd:string .

unl_KS:hasMaxTemp a owl:ObjectProperty ;
    rdfs:label "hasMaxTemp"^^xsd:string .

unl_KS:hasMinTemp a owl:ObjectProperty ;
    rdfs:label "hasMinTemp"^^xsd:string .

unl_KS:hasName a owl:ObjectProperty ;
    rdfs:label "hasName"^^xsd:string .

unl_KS:hasPrecipitation a owl:ObjectProperty ;
    rdfs:label "hasPrecipitation"^^xsd:string .

unl_KS:hasRecordingDate a owl:ObjectProperty ;
    rdfs:label "hasRecordingDate"^^xsd:string .

unl_KS:hasSnowDepth a owl:ObjectProperty ;
    rdfs:label "hasSnowDepth"^^xsd:string .

unl_KS:hasSnowfall a owl:ObjectProperty ;
    rdfs:label "hasSnowfall"^^xsd:string .

unl_KS:hasState a owl:ObjectProperty ;
    rdfs:label "hasState"^^xsd:string .

unl_KS:hasUnit a owl:ObjectProperty ;
    rdfs:label "hasUnit"^^xsd:string .

